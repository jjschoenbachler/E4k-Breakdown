<extension xmlns="http://ns.adobe.com/air/extension/22.0">
  <id>com.goodgamestudios.ane.firebase</id>
  <versionNumber>1.0</versionNumber>
  <platforms>
    <platform name="Android-ARM">
        <applicationDeployment> 
            <nativeLibrary>libFirebase.jar</nativeLibrary> 
            <initializer>com.goodgamestudios.ane.firebase.Extension</initializer>
            <finalizer>com.goodgamestudios.ane.firebase.Extension</finalizer>
        </applicationDeployment> 
    </platform>
    <platform name="Android-ARM64">
        <applicationDeployment> 
            <nativeLibrary>libFirebase.jar</nativeLibrary> 
            <initializer>com.goodgamestudios.ane.firebase.Extension</initializer>
            <finalizer>com.goodgamestudios.ane.firebase.Extension</finalizer>
        </applicationDeployment> 
    </platform>
    <platform name="Android-x86">
        <applicationDeployment> 
            <nativeLibrary>libFirebase.jar</nativeLibrary> 
            <initializer>com.goodgamestudios.ane.firebase.Extension</initializer>
            <finalizer>com.goodgamestudios.ane.firebase.Extension</finalizer>
        </applicationDeployment> 
    </platform>
    <platform name="iPhone-ARM">
        <applicationDeployment>
            <nativeLibrary>libAIRFirebase.a</nativeLibrary>
	          <initializer>AIRFirebaseExtInitializer</initializer>
            <finalizer>AIRFirebaseExtFinalizer</finalizer>
        </applicationDeployment>
    </platform>
    <platform name="default"> 
        <applicationDeployment/> 
    </platform> 
  </platforms>
</extension>
