<extension xmlns="http://ns.adobe.com/air/extension/4.0">
    <id>com.ironsource.adobeair.unityadssdk</id>
    <versionNumber>4.1.0</versionNumber>
    <platforms> 
        <platform name="Android-ARM">
            <applicationDeployment>
                <nativeLibrary>unityadssdk.jar</nativeLibrary>
                <initializer>com.ironsource.adobeair.UnityAds</initializer>
            </applicationDeployment> 
        </platform>
        <platform name="Android-ARM64">
            <applicationDeployment>
                <nativeLibrary>unityadssdk.jar</nativeLibrary>
                <initializer>com.ironsource.adobeair.UnityAds</initializer>
            </applicationDeployment> 
        </platform>
        <platform name="Android-x86">
            <applicationDeployment>
                <nativeLibrary>unityadssdk.jar</nativeLibrary>
                <initializer>com.ironsource.adobeair.UnityAds</initializer>
            </applicationDeployment> 
        </platform>
		<platform name="iPhone-ARM"> 
            <applicationDeployment> 
                <nativeLibrary>libunityadssdk.adobeair.a</nativeLibrary>
                <initializer>SSAExtensionInitializer</initializer>
            </applicationDeployment>
        </platform>
        
        <platform name="default">
            <applicationDeployment/>
        </platform>
    </platforms> 
</extension>
